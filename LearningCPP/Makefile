.PHONY: clean All

All:
	@echo "----------Building project:[ SectionEnd_ChallengeAssignment_LetterPyramid - Debug ]----------"
	@cd "SectionEnd_ChallengeAssignment_LetterPyramid" && "$(MAKE)" -f  "SectionEnd_ChallengeAssignment_LetterPyramid.mk"
clean:
	@echo "----------Cleaning project:[ SectionEnd_ChallengeAssignment_LetterPyramid - Debug ]----------"
	@cd "SectionEnd_ChallengeAssignment_LetterPyramid" && "$(MAKE)" -f  "SectionEnd_ChallengeAssignment_LetterPyramid.mk" clean
